         resetcheck.checked=false;

      }
      else{
         termsbtn.disabled="true";
         resetcheck.checked=true;

      }

   }

   function termsreset(){
      var resetcheck=document.getElementById("check-check");
      var termsbtn1=document.getElementById("black");
      var termsbtn = document.getElementById("red");
      if(termsbtn1.click){
      resetcheck.checked = false;
      termsbtn.disabled = true;}
      else
   {
      resetcheck.checked = true;
      termsbtn.disabled = false;

   }


